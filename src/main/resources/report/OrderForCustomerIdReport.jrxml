	<parameter name="custId" class="java.lang.String"/>
	<query language="SQL"><![CDATA[SELECT 
    c.name, 
    c.email, 
    c.coNo, 
    o.orderId, 
    o.date,  
    o.custId, 
    o.empId, 
    t.tireId,  
    t.description, 
    t.payment_method, 
    t.qty,  
    t.total_amount 
FROM 
    customer c
JOIN 
    orders o ON c.custId = o.custId
JOIN 
    tire_order t ON o.orderId = t.orderId
WHERE 
    o.custId = $P{custId}]]></query>
